    "express-validator": "^6.9.2",
    "moment": "^2.29.1",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "save": "^2.4.0",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0"
  },
